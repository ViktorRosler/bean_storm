

class Node
{
    Node left
    Node right
    int value
    bool has_value = False
    
    static int node_count
    
    Node(int val) 
    {
        value = val
        has_value = True
        node_count += 1
    }
    
    void add(int val)
    {
        if (!has_value)
        {
            value = val
            has_value = True
            node_count += 1
        }
        elif (val < value) 
        {
            left.add(val)
        }
        else
        {
            right.add(val)
        }
        
        
    }
}

Node tree(5)

tree.add(3)
tree.add(2)
