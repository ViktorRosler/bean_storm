#Volume of a cube
int volume(int len)
{
	return len * len * len
}

#Volume of a cylinder
float volume(float radius, float height)
{
	float ans = 3.1415926 * radius * radius * height
	ans = ans // 1
	return ans
}

#Volume of a cuboid
float volume(float len, float width, float height)
{
	return len * width * height
}


